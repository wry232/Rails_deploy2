<h3>Others who added <%= @song.artist%> - <%= @song.title%></h3>
<table>
  <thead>
    <th>Name</th>
    <th>Times Added</th>

  </thead>
  <tbody>
    <% @song.playlists.each do |p|%>

    <tr>
      <td> <a href="/users/<%= p.user.id%>"> <%= p.user.first_name%> <%= p.user.last_name%></a> </td>
      <td><%= p.times%></td>
    </tr>
    <% end %>
  </tbody>
</table>

<a href="/songs">Dashboard</a>
